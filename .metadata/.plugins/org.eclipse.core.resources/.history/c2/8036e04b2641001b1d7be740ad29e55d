package com.assesment.model;

import javax.persistence.Entity;
import javax.persistence.Table;

@Entity
@Table
public class Order {

	public String getItem() {
		return item;
	}
	public void setItem(String item) {
		this.item = item;
	}
	public long getPrice() {
		return price;
	}
	public void setPrice(long price) {
		this.price = price;
	}
	String item;
	long price;
	
}
